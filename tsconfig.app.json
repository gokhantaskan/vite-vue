{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "appconfig.d.ts",
    "env.d.ts",
    "declarations.d.ts",
    "vitest",
    "src/**/*",
    "types",
    "package.json"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
